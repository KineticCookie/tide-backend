# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

POST    /register                   controllers.AuthController.register
POST    /login                      controllers.AuthController.login
POST    /logout                     controllers.AuthController.logout(token: String)

GET     /diagram                    controllers.DataFlowController.getDataFlow(token: String, id: Option[String])
POST    /diagram                    controllers.DataFlowController.postDataFlow(token: String)
GET     /diagram/source             controllers.DataFlowController.getCompiledDataFlow(token: String, id: String)
